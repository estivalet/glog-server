<table class="table table-striped table-hover">
    <caption>List of adjuster</caption>
    <thead class="thead-light">
    <tr>
        <th>name</th>
        <th>default</th>
    </tr>
    </thead>
    <% data.machine.adjuster.forEach(function(adjuster) {%>
        <tr>
            <td>
                <%=adjuster.name%>
            </td>
            <td>
                <%=adjuster._default%>
            </td>
        </tr>
        <%}); %>
</table>
