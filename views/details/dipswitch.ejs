<table class="table table-striped table-hover">
    <caption>List of dipswitches</caption>
    <thead class="thead-light">
    <tr>
        <th>Name</th>
        <th>Value</th>
        <th>Location</th>
    </tr>
    </thead>
    <% data.dipswitch.forEach(function(dipswitch) {%>
        <tr>
            <td>
                <%=dipswitch.name%>
            </td>
            <td>
                <% dipswitch.dipvalue.forEach(function(dipvalue){ %>
                    <%=dipvalue.name%><% if(dipvalue._default == 'yes') {%> (default) <%}%><br/>
                <%}); %>
            </td>
            <td>
                <% dipswitch.diplocation.forEach(function(diplocation){ %>
                    <%=diplocation.name%> <%=diplocation.number%> <%=diplocation.inverted%><br/>
                <%}); %>
            </td>
        </tr>
        <%}); %>
</table>
