<style type="text/css">
    body{
     margin:0;
     padding:100px 0 0 150px;
    }
    div#header{
     position:absolute;
     top:0;
     left:0;
     width:100%;
     height:100px;
    }
    div#left-sidebar{
     position:absolute;
     top:100px;
     left:0;
     width:150px;
     height:calc(100% - 100px);
     overflow-y: auto;
    }
    @media screen{
     body>div#header{
      position:fixed;
     }
     body>div#left-sidebar{
      position:fixed;
     }
    }
    div#content{
    position:fixed;
     top:100px;
     width:calc(100% - 150px);
     height:calc(100% - 100px);
     overflow:auto;
    }
    body{
    font:80%/1.3 Arial,sans-serif;
   }
   h1{
    margin:0;
    padding:1em;
    font-size:1.5em;
    font-weight:bold;
   }
   h1 span{
    font-size:.667em;
   }
   h2{
    margin:0;
    padding:1em 1.25em;
    font-size:1.2em;
    font-weight:bold;
   }
   h3{
    margin:0;
    padding:1em 1.5em;
    font-size:1em;
    font-weight:bold;
   }
   pre{
    margin:.5em 1.5em;
    padding:.3em;
    color:#000;
    background:#fff;
   }
   pre,pre code,pre var{
    font:1em/1 "courier new",monospace;
   }
   pre var{
    font-style:oblique;
   }
   pre code.markup.comment{
    font-style:oblique;
    color:green;
   }
   pre code.markup.doctype{
    font-style:oblique;
    color:teal;
   }
   pre code.markup.element-name{
    font-weight: bold;
    color:purple;
   }
   pre code.markup.attribute-name{
    font-weight: bold;
    color:black;
   }
   pre code.markup.attribute-value{
    font-weight:normal;
    color:blue;
   }
   p{
    margin:0;
    padding:.5em 1.5em;
   }
   div#header{
    background:burlywood;
    color:black;
   }
   div#left-sidebar{
    background:#f1f1f1;
    color:black;
   }
   div#right-sidebar{
    background:green;
    color:black;
   }
   div#footer{
    background:olive;
    color:black;
   }
   ul {
    list-style-type: none;
    margin: 0;
    padding: 0;
    background-color: #f1f1f1;
    overflow: auto;
}

li a {
    display: block;
    color: #000;
    padding: 8px 16px;
    text-decoration: none;
}

li a.active {
    background-color: #4CAF50;
    color: white;
}

li a:hover:not(.active) {
    background-color: #555;
    color: white;
}
   </style>
     <link href="vendor/bootstrap-4.1/bootstrap.min.css" rel="stylesheet" media="all">

     <link rel="stylesheet" href="dist/css/jquery.dataTables.min.css">
   
   <div id="header">
    <p>Content can be placed hereâ€¦
   </div>
   <div id="left-sidebar">
    <ul>
            <li><a class="active" href="#home">Home</a></li>
            <li><a href="/mame/database">MAME Database</a></li>
            <li><a href="/amcp">Attract Mode Control Panel</a></li>
            <li><a href="#about">About</a></li>
          </ul>
   </div>
   <div id="content">
        <div class="container-fluid" >
                <div class="row">
                    <div class="col-xl-12">
                        <!-- RECENT REPORT 2-->
                        <div  >
                            <h3 class="title-3">recent reports</h3>
                            <table id="example" class="display compact" style="width:100%">
                                    <thead>
                                        <tr>
                                            <th>Name</th>
                                            <th>Type</th>
                                            <th>Category</th>
                                            <th>Favorite</th>
                                            <th>Kids</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <% for (var i = 0; i < systems.length; i++) { %>
                                        <tr>
                                            <td><%=systems[i].name%></td>
                                            <td><%=systems[i].type%></td>
                                            <td><%=systems[i].category%></td>
                                            <td><%=systems[i].favorite%></td>
                                            <td><%=systems[i].kids%></td>
                                        </tr>
                                        <%}%>    
                                    </tbody>
                                    <tfoot>
                                        <tr>
                                            <th>Name</th>
                                            <th>Type</th>
                                            <th>Category</th>
                                            <th>Favorite</th>
                                            <th>Kids</th>
                                        </tr>
                                    </tfoot>
                                </table>                
                            </div>
                        <!-- END RECENT REPORT 2             -->
                    </div>
                </div>
            </div>
 
        </div>
<!-- Jquery JS-->
    <script src="vendor/jquery-3.2.1.min.js"></script>
    <!-- Bootstrap JS-->
    <script src="vendor/bootstrap-4.1/bootstrap.min.js"></script>
    <script src="dist/js/jquery.dataTables.min.js"></script>
    <script>
            $(document).ready(function() {
                $('#example').DataTable();
            } );
        </script>
</body>
</html>